

      <%= form_for @post, url: post_path_url(@post_path) , html: { multipart: true } do |f| %>


          <%= f.label :name %>
          <%= f.text_field :name %>

          <%= f.label :description %>
          <%= f.text_field :description %>

          <%= f.label :review %>
          <%= f.text_field :review%>

          <%= f.label :image %>
          <%= f.file_field :image%>



      <% end %>
